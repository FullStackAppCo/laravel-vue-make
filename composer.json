{
  "name": "fsac/laravel-vue-make",
  "description": "Artisan command to generate Vue components",
  "keywords": [
    "laravel",
    "vue",
    "artisan"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Joby Harding",
      "email": "code@fullstackapp.co"
    }
  ],
  "autoload": {
    "psr-4": {
      "FullStackAppCo\\VueMake\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2|^8.0",
    "illuminate/console": "^6.0|^7.0|^8.0",
    "illuminate/filesystem": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "FullStackAppCo\\VueMake\\Support\\CommandsProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "orchestra/testbench": "^3.8|^4.0|^5.0"
  }
}
